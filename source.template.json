{
  "name": "LeanixSourceConnector",
  "config": {
    "kafka.topic": "avro",

    "chanegname.pollingInterval": 30,

    "connector.class": "LeanixSourceConnector",

    "key.converter": "io.confluent.connect.avro.AvroConverter",
